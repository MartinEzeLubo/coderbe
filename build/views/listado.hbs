<div class="container">
<table class="table">
  <thead>
    <tr>
      <th scope="col">Producto</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
    {{#each info}}
    <tr>
      <td class="align-middle">{{this.title}}</td>
      <td class="align-middle">{{this.price}}</td>
      <td class="align-middle"><img src=" {{this.thumbnail}}" alt="" style="width: 100px; height: 100px"></td>
    </tr>
    {{/each}}
    
  </tbody>
</table>

</div>

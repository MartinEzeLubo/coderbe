<div class="container">
  <table class="table">

    <tbody>
      <% info.forEach(function(product) { %>
        <tr>
          <td class="align-middle"><%= product.title %></td>
          <td class="align-middle"><%= product.price %></td>
          <td class="align-middle"><img src=<%= product.thumbnail %> alt="" style="width: 100px; height: 100px"></td>
        </tr>

      <% });  %> 
      
    </tbody>
  </table>
</div>
    
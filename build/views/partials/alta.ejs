<div class="container">
    <h1>Carga de productos</h1>
    <div class="mb-5">
        <form>
            <div class="col">
                <label for="">Producto:</label>
                <input id="title" type="text" name="title" class="form-control">
            </div>
            <div class="col">
                <label for="">Precio:</label>
                <input id="price" type="text" name="price" class="form-control">
            </div>
            <div class="col">
                <label for="">URL Imagen:</label>
                <input id="thumbnail" type="text" name="thumbnail" class="form-control">
            </div>
            <p></p>
            <button type="submit" class="btn btn-primary mt-3" onclick='saveProduct()'>Cargar producto</button>
        </form>
    </div>
</div>
